{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": [
		"config:recommended",
		":dependencyDashboard",
		":disableRateLimiting",
		":enablePreCommit",
		"github>zekihan/renovate-config:automerge-docker-digest",
		"github>zekihan/renovate-config:automerge-github-actions",
		"github>zekihan/renovate-config:commit-message",
		"github>zekihan/renovate-config:pr-labels",
		"github>zekihan/renovate-config:semantic-commits",
		"github>zekihan/renovate-config:custom-managers.json5"
	],
	"timezone": "Europe/Istanbul",
	"dependencyDashboardTitle": "Renovate Dashboard",
	"suppressNotifications": [
		"prIgnoreNotification"
	],
	"dependencyDashboardLabels": [
		"dependency/renovate"
	],
	"addLabels": [
		"dependency/renovate"
	],
	"dependencyDashboardAutoclose": false,
	"reviewers": [
		"zekihan"
	],
	"commitBodyTable": true,
	"rebaseWhen": "behind-base-branch",
	"platformCommit": "enabled",
	"configMigration": true
}
